{
  "name": "vue-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue-macros/volar": "^0.9.0",
    "eslint": "^8.36.0",
    "less": "^4.1.3",
    "taze": "^0.9.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vite-plugin-pages": "^0.29.0",
    "vitest": "^0.29.3",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            "template",
            "script",
            "style"
          ]
        }
      ],
      "no-console": "off"
    }
  }
}
