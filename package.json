{
  "name": "vue-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "VITE_CJS_IGNORE_WARNING=true vite",
    "build": "vue-tsc && VITE_CJS_IGNORE_WARNING=true vite build",
    "preview": "VITE_CJS_IGNORE_WARNING=true vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^12.8.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "2.1.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue-macros/volar": "^0.30.15",
    "eslint": "^9.22.0",
    "less": "^4.2.2",
    "taze": "^18.7.1",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.1",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-pages": "^0.32.5",
    "vitest": "^3.0.8"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            "template",
            "script",
            "style"
          ]
        }
      ],
      "no-console": "off"
    }
  }
}
